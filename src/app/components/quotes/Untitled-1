// add page with boder
function addPage() {
    doc.addPage();
    addBorder();
}

//add border to page
function addBorder() {
    doc.setLineWidth(0.5)
    doc.line(7, 8, 204, 7)
    doc.line(8, 9, 203, 8)

    doc.line(7, 285, 7, 8)
    doc.line(8, 284, 8, 9)

    doc.line(204, 285, 204, 7)
    doc.line(203, 284, 203, 8)

    doc.line(7, 285, 204, 285)
    doc.line(8, 284, 203, 284)
}

//functions sub string
function substringArray(str, len) {
    var array = (str).split(' ');
    var count = 0;
    var tempString = "";
    var perticularSubString = [];
    var flag = false;
    for (var i = 0; i < array.length; i++) {
        count += (array[i].length) + 1;
        if (count <= len) {
            flag = true;
            tempString += (array[i]) + " ";
        } else if (count >= len) {
            perticularSubString.push(tempString);
            flag = false;
            tempString = "";
            tempString = array[i] + " "
            count = 0;
        }
        if (i === array.length - 1) {
            perticularSubString.push(tempString);
        }
    }
    return (perticularSubString)
}
