    if (i <= 10) {
        doc.text("" + (i + 1), 15, 40 + (i * 18))
        if ((data.foyer[i].particulars_header).length > 16) {
            var sub_he = substringArray(data.foyer[i].particulars_header, 16)
            if (sub_he.length > 0) {
                for (var j = 0; j < sub_he.length; j++) {
